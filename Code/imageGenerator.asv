EEG_theta.data = EEG_theta.data(1:length(time_range));
EEG_alpha.data = EEG_alpha.data(1:length(time_range));
EEG_beta.data = EEG_beta.data(1:length(time_range));
EEG_gamma.data = EEG_gamma.data(1:length(time_range));


figure;
subplot(6, 1, 1);
